<h1>Productos</h1>
<button><a href="http://localhost:8080/realTimeProducts">Editar productos</a></button>
<main
  class="container"
>
  {{#each productos}}
    <section class="cardContainer">
      <h2 class="titleCard">{{this.title}}</h2>
      <img class="imgCard" src={{this.thumbnail}} alt="{{this.title}}" />
      <section class="textCard">
        <p>{{this.id}}</p>
        <p >{{this.description}}</p>
        <p>Stock: {{this.stock}} unidades</p>
        <p><span>${{this.price}}</span></p>
        <button class="btnDelete" data-id="{{this.id}}">Eliminar</button>
      </section>
    </section>
  {{/each}}

</main>

